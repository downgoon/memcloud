<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <hide_bullets_count>18</hide_bullets_count>
  <data_type>csv</data_type>
  <csv_separator>;</csv_separator>
  <skip_rows>2</skip_rows> 
  <plot_area>
    <margins>
      <left>50</left>
      <right>40</right>
      <top>55</top>
      <bottom>30</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <approx_count>8</approx_count>
    </x>
    <y_left>
      <alpha>10</alpha>
    </y_left>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>1</width>
      <color>0D8ECF</color>
    </y_left>
  </axes>
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <help>
    <button>
      <color>FCD202</color>
      <text_color>000000</text_color>
      <text_color_hover>FF0F00</text_color_hover>
    </button>
    <balloon>
      <color>FCD202</color>
      <text_color>000000</text_color>
    </balloon>
  </help>
  
  <graphs>
    <graph gid="0">
      <title>昨天</title>
      <color>EE22DD</color>
      <color_hover>EE22DD</color_hover>
      <line_width>2</line_width>
      <fill_alpha>0</fill_alpha>
      <bullet>round</bullet>
    </graph>
    <graph gid="1">
      <title>今天</title>
      <color>0000DD</color>
      <color_hover>0000DD</color_hover>
      <line_width>2</line_width>
      <fill_alpha>0</fill_alpha>
      <bullet>round</bullet>
    </graph>
    
  </graphs>
  
  
  <labels>
    <label lid="0">
      <text><![CDATA[<b>Memcached实例全天访问量曲线（每10分钟采样）</b>]]></text>
      <y>15</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>
