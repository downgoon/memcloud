<?xml version="1.0" encoding="GBK"?>
<settings>
  <data_type>csv</data_type>
  <plot_area>
    <margins>
      <left>50</left>
      <right>40</right>
      <top>50</top>
      <bottom>50</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <dashed>1</dashed>
      <dash_length>4</dash_length>
    </category>
    <value>
      <dashed>1</dashed>
      <dash_length>4</dash_length>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <legend>
    <enabled>0</enabled>
  </legend>
  <angle>0</angle>
  <column>
    <width>85</width>
    <balloon_text>{title}: {value} downloads</balloon_text>
    <grow_time>3</grow_time>
    <sequenced_grow>1</sequenced_grow>
  </column>
  
  <graphs>
    <graph gid="0">
      <title>iPhone</title>
      <color>ADD981</color>
    </graph>
    <graph gid="1">
      <title>Android</title>
      <color>7F8DA9</color>
    </graph>
    <graph gid="2">
      <title>Symbian</title>
      <color>FEC514</color>
    </graph>
  </graphs>
  
  <labels>
    <label lid="0">
      <text><![CDATA[<b>Daily Download</b>]]></text>
      <y>18</y>
      <text_color>000000</text_color>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>
